<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="report_excel_output_view_form" model="ir.ui.view">
            <field name="name">report.excel.output.form</field>
            <field name="model">report.excel.output.extend</field>
            <field name="arch" type="xml">
	            <form string="Report Output">
	                <separator string="Processing done" colspan="2"/>
	                <field name="filename" invisible="1"/>
	                <field name="filedata" readonly="1" filename="filename"/>
	                <footer>
                        <button special="cancel" string="Close" type="object" class="oe_link"/>
                    </footer>
	            </form>
            </field>
        </record>

	</data>
</odoo>